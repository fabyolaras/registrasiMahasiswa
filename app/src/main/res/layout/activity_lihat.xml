<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.larasati.registrasimahasiswa.LihatActivity">

    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/data_mhsw_recycler"
        android:layout_marginBottom="76dp">

    </android.support.v7.widget.RecyclerView>

    <Button
        android:id="@+id/button_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="29dp"
        android:layout_marginStart="39dp"
        android:onClick="kembaliMain"
        android:text="Kembali" />

    <Button
        android:id="@+id/button_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button_back"
        android:layout_alignBottom="@+id/button_back"
        android:layout_marginStart="56dp"
        android:layout_toEndOf="@+id/button_back"
        android:onClick="deleteData"
        android:text="Hapus" />

    <Button
        android:id="@+id/button_see"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button_delete"
        android:layout_alignBottom="@+id/button_delete"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="12dp"
        android:onClick="detailData"
        android:text="Lihat" />

</RelativeLayout>
